<launch>
    <node name="fw_node"
          pkg="rosserial_arduino"
          type="serial_node.py"
          output="screen"
          respawn="true">
        <!-- <param name="port" value="/dev/ttyTHS1"/> -->
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node>

    <node name="base_node"
          pkg="robot_base"
          type="base_node"/>
</launch>
